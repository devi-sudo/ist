// Function to generate a random text reply
function getRandomReply(userMessage) {
  const replies = [
    'Hi Boliye',
    'Error 69',
    'Welcome!',
    'How can I help you?'
  ];

  const randomIndex = Math.floor(Math.random() * replies.length);
  return replies[randomIndex];
}

