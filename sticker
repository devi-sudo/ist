// Handle incoming text and sticker messages
bot.on('message', async (msg) => {
  const chatId = msg.chat.id;

  try {
    // Handle text messages
    if (msg.text) {
      const userMessage = msg.text.toLowerCase();
      const reply = getRandomReply(userMessage);

      if (reply.trim() !== "") {
        await bot.sendMessage(chatId, reply, { reply_to_message_id: msg.message_id });
      } else {
        console.error("Reply message is empty");
      }
    }
  // if (msg.sticker) {
  //   // Log the sticker's file_id to the console
  //   console.log(`'${msg.sticker.file_id}',`);
  // };
    // Handle sticker messages
    if (msg.sticker) {
      const randomSticker = getRandomSticker();

      // Send a random sticker as a response
      await bot.sendSticker(chatId, randomSticker, { reply_to_message_id: msg.message_id });
    }
  } catch (error) {
    console.error('Error handling message:', error.message);
  }
});

// Log when the bot starts
console.log('Bot is running and polling for messages...' );
